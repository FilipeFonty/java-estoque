CREATE TABLE fornecedor (COD_FORNECEDOR INTEGER AUTO_INCREMENT NOT NULL, CIDADE_FORNECEDOR VARCHAR(255), UF_FORNECEDOR VARCHAR(255), NOME_FORNECEDOR VARCHAR(255), CEP_FORNECEDOR VARCHAR(255), RG_FORNECEDOR VARCHAR(255), EMAIL_FORNECEDOR VARCHAR(255), CNPJ_FORNECEDOR VARCHAR(255), SITE_FORNECEDOR VARCHAR(255), ENDERECO_FORNECEDOR VARCHAR(255), FONE_FORNECEDOR VARCHAR(255), CELULAR_FORNECEDOR VARCHAR(255), ORGAO_RG_FORNECEDOR VARCHAR(255), FAX_FORNECEDOR VARCHAR(255), BAIRRO_FORNECEDOR VARCHAR(255), CONTATO_FORNECEDOR VARCHAR(255), TIPO_FORNECEDOR CHAR(1), nomeFornecedor VARCHAR(255), CPF_FORNECEDOR VARCHAR(255), PRIMARY KEY (COD_FORNECEDOR))
CREATE TABLE banco (COD_BANCO INTEGER AUTO_INCREMENT NOT NULL, AGENCIA_BANCO INTEGER, CONTA_BANCO INTEGER, NOME_BANCO VARCHAR(255), GERENTE_BANCO VARCHAR(255), FONE_BANCO VARCHAR(255), PRIMARY KEY (COD_BANCO))
CREATE TABLE produto (COD_PRODUTO VARCHAR(255) NOT NULL, VLR_VENDA_PRODUTO DOUBLE, ESTOQUE_PRODUTO INTEGER, DESCRICAO_PRODUTO VARCHAR(255), CRITICO_PRODUTO INTEGER, VLR_COMPRA_PRODUTO DOUBLE, COD_UNIDADE INTEGER, COD_FORNECEDOR INTEGER, PRIMARY KEY (COD_PRODUTO))
CREATE TABLE cliente (COD_CLIENTE INTEGER AUTO_INCREMENT NOT NULL, PROFISSAO_CLIENTE VARCHAR(255), EMPRESA_CLIENTE VARCHAR(255), NOME_CLIENTE VARCHAR(255), FONE_EMPRESA VARCHAR(255), RG_CLIENTE VARCHAR(255), RENDA_CLIENTE DOUBLE, NASCIMENTO_CLIENTE DATE, TIPO_CLIENTE CHAR(1), CNPJ_CLIENTE VARCHAR(255), ORGAO_RG_CLIENTE VARCHAR(255), REFERENCIA_CLIENTE VARCHAR(255), CPF_CLIENTE VARCHAR(255), FONE_REFERENCIA_CLIENTE VARCHAR(255), DESDE_CLIENTE DATE, EMAIL VARCHAR(255), PRIMARY KEY (COD_CLIENTE))
CREATE TABLE unidade (COD_UNIDADE INTEGER AUTO_INCREMENT NOT NULL, DESCRICAO_UNIDADE VARCHAR(255), PRIMARY KEY (COD_UNIDADE))
ALTER TABLE produto ADD CONSTRAINT FK_produto_COD_FORNECEDOR FOREIGN KEY (COD_FORNECEDOR) REFERENCES fornecedor (COD_FORNECEDOR)
ALTER TABLE produto ADD CONSTRAINT FK_produto_COD_UNIDADE FOREIGN KEY (COD_UNIDADE) REFERENCES unidade (COD_UNIDADE)
